                                <DatePickerTextBox.Template>
                                    <ControlTemplate>
                                        <Border Background="{TemplateBinding Background}">

                                            <ScrollViewer x:Name="PART_ContentHost" VerticalAlignment="Center"  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>

